{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "SmtpSettings": {
    "SmtpServer": "smtp.example.com",
    "SmtpPort": 587, // Обычно порт 587 для TLS или 465 для SSL
    "EnableSsl": true, // Установите в true, если используется SSL
    "Username": "your_username",
    "Password": "your_password"
  },
  "AllowedHosts": "*"
}